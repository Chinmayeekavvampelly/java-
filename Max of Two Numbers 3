import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Long A,B;
        Scanner s=new Scanner(System.in);
        A=s.nextLong();
        B=s.nextLong();
        if(A>B){
            System.out.println(A);
        }
        else{
            System.out.println(B);
        }
    }
}
