import java.util.*;
class Test{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        Arrays.sort(a);
        int mid=n/2;
        for(int i=n-1;i>=mid;i--){
            System.out.print(a[i]+" ");
        }
        for(int i=0;i<=mid-1;i++){
            System.out.print(a[i]+" ");
        }
    }
}
